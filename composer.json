{
    "name": "deek87/c5browser",
    "description": "A concrete5 web browser module for codeception",
    "type": "library",
    "require": {
        "php": ">=5.5.9",
        "codeception/codeception": "^4.1",
        "codeception/module-webdriver": "^1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "autoload": {
        "psr-4": {
            "Codeception\\Module\\" : "src"
        }
    },
    "autoload-dev": {
        "C5Browser\\Test": "tests"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Derek Cameron",
            "email": "info@derekcameron.com"
        }
    ],
    "minimum-stability": "stable",
    "scripts": {
        "phpcs:lint" : "vendor/bin/php-cs-fixer fix --dry-run",
        "phpcs:fix" : "vendor/bin/php-cs-fixer fix"
    }
}
